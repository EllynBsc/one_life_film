var line = document.getElementById('playline-id[<%= @playline.id %>]');
<% if @playline.errors.any? %>
  line.innerHTML = "<%= j render 'playlines/form', playbook: @playbook, playline: @playline, times: @times %>";
<% else %>
  line.outerHTML =  "<%= j render 'playlines/show', playline: @playline %>";
  sortTable();
<% end %>
