<table class="table table-striped table-hover">
  <thead z>
    <tr>
      <th><h2><%= @title %></h2 id="booking-title"></th>
    </tr>
  </thead>

  <thead class="thead-inverse">
    <tr>
      <th>Name</th>
      <th>Contact</th>
      <th>Location</th>
      <th>Songs</th>

      <th>Playlines</th>

      <th>Status</th>

    </tr>
  </thead>
  <tbody>
    <% bookings.each do |booking| %>
      <%= render 'show', booking: booking %>
  <% end %>
</tbody>
</table>
